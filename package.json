{
  "name": "final-space-fan-site",
  "version": "0.0.1",
  "description": "A cool website about one of the best animated scy-fy series.",
  "browserslist": "cover 99% in ES, last 5 versions, not dead",
  "source": [
    "src/cover/index.html",
    "src/category/category.html",
    "src/details/details.html",
    "src/links/links.html",
    "src/presentation/presentation.html"
  ],
  
  "scripts": {
    "cover": "parcel src/cover/index.html",
    "presentation": "parcel src/presentation/index.html",
    "details": "parcel src/details/index.html",
    "links": "parcel src/links/index.html",
    "category": "parcel src/category/index.html",
    "start": "npm-run-all clean parcel:dev",
    "build": "npm-run-all clean parcel:build",
    "parcel:dev": "parcel",
    "parcel:build": "parcel build",
    "clean": "rimraf dist .parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpalomarbl/final-space-fan-site.git"
  },
  "keywords": [
    "scy-fy",
    "final",
    "space",
    "series",
    "cartoon",
    "website"
  ],
  "author": "Jaime Sime√≥n Palomar Blumenthal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jpalomarbl/final-space-fan-site/issues"
  },
  "homepage": "https://github.com/jpalomarbl/final-space-fan-site#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel": "^2.10.0",
    "posthtml-include": "^1.7.4",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "axios": "^1.5.1"
  }
